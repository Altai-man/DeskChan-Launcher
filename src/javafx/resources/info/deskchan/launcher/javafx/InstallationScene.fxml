<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane minWidth="200" minHeight="150" prefWidth="350" prefHeight="250" fx:controller="info.deskchan.launcher.javafx.InstallationController"
            xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
    <ListView fx:id="logListView" items="${controller.logList}"
              AnchorPane.bottomAnchor="75.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0" />
    <ProgressBar progress="${controller.progress}"
                 AnchorPane.bottomAnchor="45.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" />
    <Button cancelButton="true" mnemonicParsing="false" onAction="#quit" text="%button.cancel"
            AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" />

    <stylesheets>
        <URL value="@styles.css" />
    </stylesheets>
</AnchorPane>
